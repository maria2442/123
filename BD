#include <iostream>
#include<string>
#include <fstream>
#include<vector>
using namespace std;

class Table{
public:
    vector <vector <string> > tab[10][10];
    //int tab[100][100];
    int col,str;
    int zag1[100];
    string zag[100];
    virtual void newst()=0;
    //void delst();
    void show();
};
void Table::show()
{
    int n;
    cin >> n;
    for (int i = 0; i < col; i++)
    {
        for (int k = zag[i].size(); k <= zag1[i]; k++)
        {
            cout << " ";
        }
        cout << zag[i] << " | ";
    }
    cout << endl;
    for (int i = 0; i <col; i++)
    {
        for (int k = tab[n][i].size(); k <= zag1[i]; k++)
                cout << " ";
            cout << tab[n][i] << " | ";

    }
    cout << endl;

}
class Students: public Table
{
   public:
    void newst();
    Students();
};
Students::Students()
{
    zag[0]="";zag1[0]=20;
    zag[1]="";zag1[1]=20;
    zag[2]="";zag1[2]=20;
    zag[3]="";zag1[3]=3;
    zag[4]="";zag1[4]=3;
    ifstream fin;
    int i = 0;
    string a;

    fin.open("students.txt");
    while (!fin.eof())
    {
        for(int j=0;j<5;j++)
        {
            fin>>a;
            cout<<endl<<i<<endl<<tab.size()<<endl;
            if(i<tab.size())
            {
                cout<<"++";
                tab[i].push_back(a);
            }
        }
        i++;
    }
    fin.close();
    col = 4;
    str = i-1;
}
void Students::newst()
{
    string a;
    for(int j=0;j<5;j++)
        cin>>tab[str][j];
    str++;
}

